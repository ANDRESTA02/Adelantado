<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="field col-md-3">
    <%= f.label :Nombre %><br />
    <%= f.text_field :nombre %>
  </div>

  <div class="field col-md-3 ">
    <%= f.label :Apellidos %><br />
    <%= f.text_field :apellido %>
  </div>

  <div class="field col-md-3">
    <%= f.label :Telefono %><br />
    <%= f.text_field :telefono %>
  </div>

  <div class="field col-md-3">
    <%= f.label :Celular %><br />
    <%= f.text_field :celular %>
  </div>

  <div class="field col-md-3">
    <%= f.label :Direccion %><br />
    <%= f.text_field :dir %>
  </div>

  <div class="field col-md-3">
   	<%= f.label :Email %><br />
   	<%= f.text_field :email %>
  </div>

  <div class="field col-md-3">
 	<%= f.label :Contraseña %><br />
 	<%= f.password_field :password %>
  </div>

  <div class="field col-md-3">
 	<%= f.label :Confirmar_Contraseña %><br />
 	<%= f.password_field :password_confirmation %>
  </div>

  <div class=".actions ">
  <%= f.submit"Guardar" %>
  </div>

<% end %>